<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RegularExpressionProtection async="false" continueOnError="false" enabled="true" name="HPX_URIRegexProtection">
    <DisplayName>HPX_URIRegexProtection</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <URIPath>
        <Pattern ignoreCase="false">.*Exception in thread.*</Pattern>
        <Pattern ignoreCase="false">&lt;\s*script\b[^&gt;]*&gt;[^&lt;]+&lt;\s*\/\s*script\s*&gt;</Pattern>
        <Pattern ignoreCase="false">n\s*\\\\\s*slash</Pattern>
        <Pattern ignoreCase="false">n\s*\/\s*slash</Pattern>
        <Pattern ignoreCase="false">n\s*\\"\s*quotes</Pattern>
        <Pattern ignoreCase="false">n\s*\\b\s*space</Pattern>
        <Pattern ignoreCase="false">n\s*\\f\s*forwardfeed</Pattern>
        <Pattern ignoreCase="false">n\s*\\n\s*newline</Pattern>
        <Pattern ignoreCase="false">n\s*\\r\s*carria</Pattern>
        <Pattern ignoreCase="false">n\s*\\t\s*tab</Pattern>
        <Pattern ignoreCase="false">n\s*\\uFFFF\s*hex</Pattern>
        <Pattern ignoreCase="false">&lt;!--#(include|exec|echo|config|printenv)\s+.*</Pattern>
        <Pattern ignoreCase="false">/?(ancestor(-or-self)?|descendant(-or-self)?|following(-sibling))</Pattern>
        <Pattern ignoreCase="false">&lt;\s*script\b[^&gt;]*&gt;[^&lt;]+&lt;\s*/\s*script\s*&gt;</Pattern>
    </URIPath>
    <Source>request</Source>
</RegularExpressionProtection>