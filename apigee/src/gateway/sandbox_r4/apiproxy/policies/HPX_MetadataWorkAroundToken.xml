<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="HPX_MetadataWorkAroundToken">
    <DisplayName>HPX_MetadataWorkAroundToken</DisplayName>
    <Properties/>
