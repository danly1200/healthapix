<!-- custom style for Author by Humana -->
<style>
.btn-block-pad {
  padding: 13px 50px 13px 50px !important;
}
.humana-h3 {
  font-family: "Libre Baskerville", serif;
  color: rgba(59, 37, 78, 1);
  font-size: 24px;
  font-weight: bold;
  line-height: 1.5;
  text-align: left;
}
.h-text {
  font-family: "Varela Round", sans-serif;
  text-align: left;
  color: rgba(69, 77, 79, 1);
}
/* Buttons */
.button {
  font-family: "Varela Round", sans-serif;
  font-weight: bold;
  background-color: #ff854d;
  border: none;
  color: white;
  padding: 10px 24px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  transition-duration: 0.2s;
  cursor: pointer;
}

.button1 {
  background-color:#ff854d; 
  color: black; 
  border-radius: 4px;
}

.button1:hover {
  background-color: rgb(255, 81, 0);
  color: white;
}

.button2 {
  background-color: white; 
  color: rgb(204, 65, 0); 
  text-decoration: underline;
}

.button2:hover {
  color: rgb(59, 37, 78);
}

.link {
  font-weight: bold;
  background-color: white; 
  color: rgb(204, 65, 0); 
  text-decoration: underline;
}

.link:hover {
  color: rgb(59, 37, 78);
}

.ul-h {
  list-style: outside;
  margin: 0;
  font-family: "Varela Round", sans-serif;
  color: rgba(69, 77, 79, 1);
}

#pinned {
  position: fixed;
  bottom: 0;
  width: 298px;
  background-color: #ffffff;
  border-top: 2px solid lightgray;
  border-bottom: 5px solid #ff854d;
  } 
</style>

</head>

