    </header>
    <!-- logged in user -->
    <div class="user-info clearfix" style="display: <%=showUserInfoDiv?'visible':'none' %>">
      <% if(logged_in_user_email != null && logged_in_user_email != "") { %>
      <% if(logged_in_user_image == null || logged_in_user_image == "") {
               logged_in_user_image = req.query.basepath+"/ui/images/avatar.png"
                }%>
      <div class="logged-in-container">
       <div style="width: 15%;" align="center"><img style="width: 40px;" src="<%= logged_in_user_image%>"></div>
        <div class="details">
          <p class="user-name"><%= logged_in_user_first_name%> <%= logged_in_user_surname%></p>
          <p class="user-email"><%= logged_in_user_email%></p>
        </div>
