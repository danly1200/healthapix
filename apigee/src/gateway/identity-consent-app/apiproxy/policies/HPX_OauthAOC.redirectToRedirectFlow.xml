<?xml version="1.0" encoding="UTF-8"?><AssignMessage name="HPX_OauthAOC.redirectToRedirectFlow">
	<DisplayName>OauthAOC.RedirectToRedirectFlow</DisplayName>
	<AssignTo createNew="true" type="response">response</AssignTo>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<Set>
		<StatusCode>302</StatusCode>
		<Headers> 
			<Header name="Location">{appCallBackUrl}&amp;state={req_state}</Header> 
			<Header name="Access-Control-Allow-Origin">*</Header>
		</Headers> 
	</Set>
</AssignMessage>
